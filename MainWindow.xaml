<Window x:Class="Layout.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Layout"
        mc:Ignorable="d"
        Title="MainWindow" Height="229" Width="187">
    <Grid>
        <Button x:Name="myGrid" Content="Grid" HorizontalAlignment="Left" Margin="40,28,0,0" VerticalAlignment="Top" Click="myGrid_Click"/>
        <Button x:Name="myStacPanel" Content="Stack Panel" HorizontalAlignment="Left" Margin="40,53,0,0" VerticalAlignment="Top" Click="myStacPanel_Click"/>
        <Button x:Name="myDockPanel" Content="Dock Panel" HorizontalAlignment="Left" Margin="40,78,0,0" VerticalAlignment="Top" Click="myDockPanel_Click"/>
        <Button x:Name="myWrapPanel" Content="Wrap Panel" HorizontalAlignment="Left" Margin="40,103,0,0" VerticalAlignment="Top" Click="myWrapPanel_Click"/>
        <Button x:Name="myCanvas" Content="Canvas" HorizontalAlignment="Left" Margin="40,128,0,0" VerticalAlignment="Top" Click="myCanvas_Click"/>

    </Grid>
</Window>
